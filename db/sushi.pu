@startuml
title db_sushi
entity "商品" {
  id
  商品名
  種別id(FK)
  金額
  持ち帰りフラグ
}

entity "種別" {
  id
  種別(セット,単品など)
}


entity "注文詳細" {
  id
  商品id(FK)
  注文票id(FK)
  わさび抜
  個数
}

entity "注文票" {
  id
  顧客id(FK)
  コメント
  合計皿数
  注文日時
  決済フラグ
}

entity "顧客" {
  id
  氏名
  電話番号
}

"商品" }- "種別"
"顧客" -{ "注文票"
"注文票" -{ "注文詳細"
"注文詳細" }- "商品"
@enduml
